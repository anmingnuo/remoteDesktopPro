<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.remotedesktop.mapper.PCRecordMapper">
   <select id="pcRecordByMacAddress" resultType="PCRecord">
      select * from pc_record where mac_address=#{macAddress}
   </select>
</mapper>
